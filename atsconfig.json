{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "jsx": "react",
    "incremental": true,
    "declaration": false,
    "sourceMap": false,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "strict": true,
    "alwaysStrict": false,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {},
    // "rootDir": "",
    "typeRoots": [
      "./src"
    ],
    "types": [
      "node"
    ],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "include": [],
  "exclude": []
}
